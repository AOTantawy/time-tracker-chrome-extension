{
  "name": "Time Tracker",
  "description": "Track Your time",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/clock16.png",
      "32": "/icons/clock32.png"
    }
  },
  "icons": {
    "16": "/icons/clock16.png",
    "32": "/icons/clock32.png"
  }
}
