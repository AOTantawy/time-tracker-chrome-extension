{
  "name": "Time Tracker",
  "description": "Track Your time",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "/icons/clock32.png",
      "64": "/icons/clock64.png",
      "128": "/icons/clock128.png"
    }
  },
  "icons": {
    "32": "/icons/clock32.png",
    "64": "/icons/clock64.png",
    "128": "/icons/clock128.png"
  }
}
